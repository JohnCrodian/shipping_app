<h1>Jobs#show</h1>
<p>Find me in app/views/jobs/show.html.erb</p>

<% @jobs.each do |job| %>
	<p>Job Number: <%= job.id %></p>
	<p>Description: <%= job.description %></p>
	<p>Cost: <%=job.cost %></p>
	<p>Origin: <%= job.origin %></p>
	<p>Destination: <%= job.destination %></p>
	<p>Containers Needed: <%= job.cont_needed %></p>
<% end %>